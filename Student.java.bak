
// when we do not override object class equals() method it just check reference of two object



class Student 
{
	
	String name;
	int roll;

	Student(String name,int roll){
	this.name=name;
	this.roll=roll;
	
	
	
	}

	
	
	public static void main(String[] args) 
	{
		
		Student s1=new Student("Noman", 101);
		
		Student s2=new Student("Murad", 102);
		
		Student s3=new Student("Noman", 101);
		
		Student s4=s1;
		
		System.out.println(s1.equals(s2));
		System.out.println(s1.equals(s3));
		System.out.println(s1.equals(s4));
	}
}
